data item {
    @key
    id string
    @index
    name string
    x int 
    y int 
    z int 
    weight int 
    is_open bool
    numbers list[int]
    dictionary map[string,int]
} with { 
    get 
    post 
    put 
    delete 
} query {
    qry1: where item.x < item.y orderby item.weight
    qry2: where item.z == 10
} permission {
    user: read, write
    ogr2: read
} 

function get_item(id int) return item
